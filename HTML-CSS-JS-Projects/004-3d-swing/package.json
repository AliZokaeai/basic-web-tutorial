{
  "name": "Swing-3D",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "fuse:watch": "fuse -i ./main/main.html -o ./dist/output-watch.html -w",
    "fuse:build": "fuse -i ./main/main.html -o ./dist/index.html ",
    "watch:sass": "sass scss/main.scss:css/style.css --watch",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass fuse:watch",
    "compile:sass": "sass scss/main.scss:css/style.comp.css --no-source-map",
    "prefix:css": "postcss css/style.comp.css --use autoprefixer --no-map -o css/style.prefix.css",
    "minify:css": "postcss css/style.prefix.css -u cssnano -o css/style.css",
    "build:css": "npm-run-all compile:sass prefix:css minify:css fuse:build"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "cssnano": "^7.1.2",
    "fuse": "^0.4.0",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^11.0.1",
    "sass": "^1.32.8"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
