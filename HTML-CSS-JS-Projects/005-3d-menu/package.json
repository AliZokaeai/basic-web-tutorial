{
  "name": "3d-menu",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "fuse-html:watch": "fuse -i ./main/main.html  -o ./dist/index.html  -w",
    "fuse-scripts:watch": "fuse -i  ./modules/main.js -o ./scripts/scripts.js -w",
    "fuse-html:build": "fuse -i ./main/main.html -o ./dist/index.html -c",
    "fuse-script:build": "fuse -i ./modules/main.js -o ./scripts/scripts.js",
    "watch:sass": "sass scss/main.scss:css/style.css --watch",
    "devserver": "live-server  ",
    "start": "npm-run-all --parallel  watch:sass fuse-html:watch fuse-scripts:watch devserver",
    "compile:sass": "sass scss/main.scss:css/style.comp.css --no-source-map",
    "prefix:css": "postcss css/style.comp.css --use autoprefixer --no-map -o css/style.prefix.css",
    "minify:css": "postcss css/style.prefix.css -u cssnano -o css/style.css",
    "build": "npm-run-all compile:sass prefix:css minify:css fuse-html:build fuse-script:build"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "cssnano": "^7.1.2",
    "fuse": "^0.4.0",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^11.0.1",
    "sass": "^1.32.8"
  },
  "keywords": [],
